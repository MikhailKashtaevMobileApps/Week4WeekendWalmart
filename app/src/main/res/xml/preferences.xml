<?xml version="1.0" encoding="utf-8"?>

<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <EditTextPreference
        android:key="zip"
        android:title="Zip"
        android:summary=""
        android:dialogTitle="Zip Code"
        android:defaultValue="08865"/>
    <ListPreference
        android:key="units"
        android:title="Units"
        android:summary="%s"
        android:entries="@array/units_entries"
        android:entryValues="@array/units_entries"
        android:dialogTitle="Units"
        android:defaultValue="Celcius"/>
</PreferenceScreen>
